<script type="text/javascript">
// eslint-disable-next-line no-unused-vars
import Vue from "vue";
export default {
  // 不要忘记了 name 属性
  name: "blacklist",
  // 变量
  data () {
    return {
      MoneyBigDataReport: {
        s1: {
          data: {},
          result: [
            {
              "hit": [],
              "riskType": "信贷不良记录"
            }, {
              "hit": [],
              "riskType": "身份异常记录"
            }, {
              "hit": [],
              "riskType": "行为异常记录"
            }
          ]
        },
      },
      Recordata: [
        {
          name:
            "1.通过身份证号码查询，用户被相关机构合作方信贷风控部门评为低风险用户。",
          id: "1"
        },
        {
          name:
            "2.通过身份证号查询，用户被现金分期合作方信贷风控部门评委低风险用户。",
          id: "2"
        }
      ]
    };
  },
  computed: {},
  // 使用其它组件
  components: {},
  // 方法
  watch: {},
  beforeCreate () { },
  methods: {
    tobak () {
      this.$router.push("/bigdatalist/bigdatahome");
    },
    bigdatalook () {
      this.$router.push("/bigdatalist/Summary");
    }
  },
  // 生命周期函数
  beforeCreate () { },
  mounted () { },
  // 每次进路由会调用这个方法
  activated () {
    var MoneyBigDataReport = this.getLocalStorage('MoneyBigDataReport');
    if (MoneyBigDataReport) {
      MoneyBigDataReport = JSON.parse(MoneyBigDataReport);
      console.log(MoneyBigDataReport)
      var datas = MoneyBigDataReport.data
      var s1 = datas.s1;
      //处理字符串替换
      var reg = /{([^\s]*?)}/g, ret2 = [], arr;
      var cbody = datas.s1.summary
      while (arr = reg.exec(cbody)) {
        ret2.push(arr[1]);
        cbody = cbody.replace(arr[0], s1.data[arr[1]])
      }
      datas.s1.summary = cbody
      this.MoneyBigDataReport = datas;
    } else {
      this.routerTo('index')
    }
  }
};
</script>

